<%
    let exampleImplementations = typeof examples !== 'undefined' ? examples : [
        {
            name: 'handlebars'
        }
    ];
%>

<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Webpack Base - Demo</title>
</head>
<body>
    <ul>
	<%exampleImplementations.forEach((exampleMetadata) => {%>
    	<li><a href="./<%= exampleMetadata.name%>/index.html"><%= exampleMetadata.name%></a></li>
	<%});%>
    </ul>
</body>
</html>
